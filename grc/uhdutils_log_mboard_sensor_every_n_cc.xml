<?xml version="1.0"?>
<block>
  <name>Log MBoard Sensor Every N</name>
  <key>uhdutils_log_mboard_sensor_every_n_cc</key>
  <category>uhdutils</category>
  <import>import uhdutils</import>
  <make>uhdutils.log_mboard_sensor_every_n_cc($dev_addr, $sensor_name, $n, $file_name)</make>
  <param>
    <name>Device Addr</name>
    <key>dev_addr</key>
    <value>""</value>
    <type>string</type>
  </param>
  <param>
    <name>Sensor Name</name>
    <key>sensor_name</key>
    <type>string</type>
  </param>
  <param>
    <name>N</name>
    <key>n</key>
    <type>int</type>
  </param>
  <param>
    <name>File</name>
    <key>file_name</key>
    <type>file_save</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
